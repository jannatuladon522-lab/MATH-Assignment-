//Task 2:Check Whether a Matrix is Symmetric
#include <iostream>
using namespace std;
int main() {
    int n;
    cout << "Enter size of square matrix: ";
    cin >> n;
    int A[n][n];
    cout << "Enter matrix elements:\n";
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            cin >> A[i][j];
        }
    }
    bool symmetric = true;
    for (int i = 0; i < n; i++) {
        for (int j = i + 1; j < n; j++) {
            if (A[i][j] != A[j][i]) {
                symmetric = false;
                break;
            }
        }
        if (!symmetric) break;
    }
    if (symmetric)
        cout << "Matrix is symmetric.\n";
    else
        cout << "Matrix is not symmetric.\n";

    return 0;
}
